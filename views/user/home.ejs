<div class="banner-area">
  <div class="container-fluid">
    <div class="row align-items-center">
      <!-- Banner Section -->
      <div class="col-lg-12">
        <div class="banner-wrap">
          <!-- Image -->
          <img
            src="/assets/img/image/banner1.jpg"
            alt="Banner"
            class="banner-image"
          />
          <!-- Text and Button -->
          <div class="banner-text-overlay">
            <h2 class="banner-title">Discover Premium Headphones</h2>
            <p class="banner-subtitle">Experience unmatched sound quality</p>
            <a href="#" class="banner-button">Explore Now</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>







